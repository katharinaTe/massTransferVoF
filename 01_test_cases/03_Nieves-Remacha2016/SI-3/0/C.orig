FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      C;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 1 0 0];

internalField   uniform 0;

boundaryField
{
    liquid
    {
        type            fixedValue;
        value           uniform 1;
    }

    gas
    {
        type            zeroGradient;
        value           uniform 0;
    }

    frontAndBack
    {
        type            empty;
    }
}

