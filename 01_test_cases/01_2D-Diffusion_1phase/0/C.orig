FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      C;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 1 0 0];

internalField   uniform 0;

boundaryField
{
    top
    {
        type            empty;
    }

    bottom
    {
        type            empty;
    }

    walls
    {
        type            zeroGradient;
    }
}


